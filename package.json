{
  "name": "sweet-modal-vue",
  "type": "module",
  "files": ["dist"],
  "version": "2.1.0",
  "description": "SweetModal library for use with Vue.js",
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.1",
    "sass": "^1.79.5",
    "vite": "^5.4.9",
    "vue": "^2.7",
    "vue-highlightjs": "^1.1.0",
    "vue-template-compiler": "^2.4.4"
  },
  "peerDependencies": {
    "vue": "2.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "wk-dev": "webpack-dev-server --config docs/webpack.config.js --inline --hot",
    "wk-build": "npm run build-src && npm run build-docs",
    "wk-build-src": "webpack --config src/webpack.config.js --progress --hide-modules",
    "wk-build-docs": "webpack --config docs/webpack.config.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adeptoas/sweet-modal-vue.git"
  },
  "keywords": [
    "sweetmodal",
    "modal",
    "vue",
    "library"
  ],
  "main": "dist/sweet-modal.es.js",
  "module": "src/main.js",
  "exports": {
      ".": {
          "import": "./dist/sweet-modal.es.js",
          "require": "./dist/sweet-modal.umd.js"
      }
  },
  "author": "Adepto.as AS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adeptoas/sweet-modal-vue/issues"
  },
  "homepage": "https://github.com/adeptoas/sweet-modal-vue#readme"
}
